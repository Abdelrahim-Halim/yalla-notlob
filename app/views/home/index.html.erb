<div class="container">
	<div class="row">
		<div class="col-sm-6">
			<div class="panel panel-primary">
			    <div class="panel-heading">
			        Latest Orders
			    </div>
			    <div class="panel-body">
			        <div class="row">
			            <div class="col-lg-12">
			            <% @orders.each do |order| %>
							<b>
							
							<% if(order.order_type == "breakfast") %>
							    BreakFast at <%= order.created_at %></b><br>
                                <% elsif(order.order_type == "lunch")%>
                                Lunch at <%= order.created_at %></b><br>
							    <% else %>
							    Dinner at <%= order.created_at %> </b><br>
							  <% end %>
						
						<% end %>  
			            </div>
			        </div>
			    </div>
			</div>
		</div>


		

	</div>
</div>